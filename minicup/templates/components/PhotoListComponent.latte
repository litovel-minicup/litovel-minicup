{foreach $photos as $photo}
    <a class="swipebox" title="{$iterator->counter}. z {$photos|length}" href={plink :Media:medium $photo->filename} >
        <img src="{plink :Media:small $photo->filename}" alt="image">
    </a>
{/foreach}
{if !$photos}
    <p>Žádné fotky k zobrazení.</p>
{/if}

<script>
    jQuery(function ($) {
        $( '.swipebox').off('click').swipebox();
    });
</script>