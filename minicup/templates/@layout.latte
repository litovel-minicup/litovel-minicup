    <title n:snippet="title">{ifset title}{include title|stripHtml} | {/ifset}
        MINICUP {$category->year->slug}</title>

    {block seo}
        <!-- SEO -->
        <meta property="description"
              content="Portál pro turnaj v miniházené MINICUP{if $category->year->actual} 2017 konaný ve dnech 9. - 11. června 2017{else} {$category->year->slug}{/if} v Litovli. Celkem nás čeká 132 zápasů v dvou kategoriích, autogramiáda extraligového družstva TJ Tatranu Litovel a spousta dalších zajímavých aktivit.">
        <meta property="keywords" content="litovel,hanacke,benatky,minihazena,hazena,tatran,extraliga,minicup">
        <meta property="author" content="Josef Kolář, Matěj Mitaš">
        {block og}
            <!-- OG -->
            <meta property="fb:app_id" content="1709323006015727">
            <meta property="og:type" content="website">
            <meta property="og:site_name" content="Litovel MINICUP">
            <meta property="og:description"
                  content="Portál pro turnaj v miniházené MINICUP{if $category->year->actual} 2017 konaný ve dnech 9. - 11. června 2017{else} {$category->year->slug}{/if} v Litovli. Celkem nás čeká 132 zápasů v dvou kategoriích, autogramiáda extraligového družstva TJ Tatranu Litovel a spousta dalších zajímavých aktivit.">
            <meta property="og:url" content="{$absoluteUrl}">
            <meta property="og:title"
                  content="{ifset title}{include title|stripHtml} | {/ifset}MINICUP {$category->year->slug}">
            {block ogImage}
